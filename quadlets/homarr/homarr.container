[Unit]
Description=Homarr
After=network-online.target

[Service]
Restart=on-failure
TimeoutStartSec=900

[Install]
WantedBy=default.target

[Container]
Image=ghcr.io/ajnart/homarr:latest
ContainerName=homarr
HostName=homarr

Volume=homarr-configs:/app/data/configs
Volume=homarr-icons:/app/public/icons
